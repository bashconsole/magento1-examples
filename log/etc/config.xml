<?xml version="1.0"?>
<config>
    <modules>
        <Hunter_Log>
            <version>0.1.0</version>
        </Hunter_Log>
    </modules>
    <global>
        <models>
          <log>
              <rewrite>
                  <log>Hunter_Log_Model_Log</log>
              </rewrite>
          </log>
          <log_resource>
              <rewrite>
                  <log>Hunter_Log_Model_Resource_Log</log>
              </rewrite>
	       <entities>                    
                    <sales_quote_table>
                        <table>sales_flat_quote</table>
                    </sales_quote_table>
                    <sales_quote_item_table>
                        <table>sales_flat_quote_item</table>
                    </sales_quote_item_table>
                    <sales_quote_item_option_table>
                        <table>sales_flat_quote_item_option</table>
                    </sales_quote_item_option_table>    
                    <sales_quote_address_table>
                        <table>sales_flat_quote_address</table>
                    </sales_quote_address_table>
                    <sales_quote_address_item_table>
                        <table>sales_flat_quote_address_item</table>
                    </sales_quote_address_item_table>
		    <sales_quote_payment_table>
                        <table>sales_flat_quote_payment</table>
                    </sales_quote_payment_table>
                    <sales_quote_shipping_rate_table>
                        <table>sales_flat_quote_shipping_rate</table>
                    </sales_quote_shipping_rate_table>
                </entities>
          </log_resource>
        </models>
    </global>
</config>
